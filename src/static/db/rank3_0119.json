    "playerMap": { "p1": { "name": "马克", "hwa": [186, 73, 19], "title": "路人王12冠王-“答案”\n", "playerId": "p1", "info": "年仅19岁，是路人王唯一一位从S1到S5赛季都能拿到冠军的选手。" }, "p2": { "name": "王晶", "hwa": [190, 90, 25], "title": "路人王S4宁波站冠军\n", "playerId": "p2", "info": "前山西财经大学队长，球风强硬，拥有出色的对抗和全面的进攻技巧。" }, "p3": { "name": "张梓祎", "hwa": [188, 90, 27], "title": "前CUBA河南大学核心\n", "playerId": "p3", "info": "CUBA河南大学队史第一人，被称为“洛阳球王”" }, "p4": { "name": "张帅康", "hwa": [183, 75, 21], "title": "路人王4冠王-变向狂魔", "playerId": "p4", "info": "路人王长沙站冠军，变向狂魔，拥有极快的速度和令人瞠目结舌的大幅度变向。" }, "p5": { "name": "陈凯涛", "hwa": [180, 82, 23], "title": "路人王5冠王-川渝霸主\n", "playerId": "p5", "info": "路人王18年川渝地区新霸主，虽然身高不高，但拥有十分全面的技巧，攻防两端均有极高的水准。" }, "p6": { "name": "黄宇军", "hwa": [193, 120, 28], "title": "路人王S5上海站冠军\n", "playerId": "p6", "info": "拥有超级内线一样BUG的体格，以及后卫般的运球和手感。" }, "p7": { "name": "李俊达", "hwa": [187, 75, 24], "title": "路人王河南6站亚军\n", "playerId": "p7", "info": "被誉为路人王“小杜兰特”身高臂展出众，球风飘逸。" }, "p8": { "name": "宋流轩", "hwa": [178, 70, 23], "title": "S2-S4八冠王-“打铁哥”\n", "playerId": "p8", "info": "路人王中投小王子，ID“打铁不算多”，可能是路人王中投最准的球员，也是路人王小个子球员代表。" }, "p9": { "name": "徐长龙", "hwa": [188, 78, 23], "title": "CUBA山西大学核心", "playerId": "p9", "info": "CUBA山西大学核心，西北赛区MVP，拥有CUBA里顶级的外线防守能力和极快的速度。" }, "p10": { "name": "张天宇", "hwa": [180, 70, 22], "title": "路人王S5南昌站冠军\n", "playerId": "p10", "info": "CUBA南昌大学的球员，拥有风骚的运球，扎实的突投结合能力。" }, "p11": { "name": "张智扬", "hwa": [190, 85, 23], "title": "路人王3冠王\n西安体育学院队长\n", "playerId": "p11", "info": "路人王西安站冠军，西安体院队长，能轻松上演实战劈扣，拥有高超的球商和实战经验。" }, "p12": { "name": "孟亚东", "hwa": [190, 100, 25], "title": "路人王46冠王-头盔哥\n", "playerId": "p12", "info": "路人王初代大魔王之一，可能是街球和实战结合的最好的球员。" }, "p13": { "name": "郝天佶", "hwa": [196, 95, 40], "title": "深圳站4站4冠-前CBA名将\n", "playerId": "p13", "info": "40岁，前CBA东莞猎豹队队长，NBL总冠军核心成员，路人王冠军赛冠军，实力榜第一人！" }, "p14": { "name": "陈泽文", "hwa": [179, 74, 25], "title": "武汉4冠霸主-武汉直升机\n", "playerId": "p14", "info": "外号武汉直升机，惊人的弹跳可以轻松上演实战暴扣。精准的三分球是他多次完成逆转和以小胜大的关键。" }, "p15": { "name": "耿骁骐", "hwa": [172, 68, 25], "title": "路人王8冠王-军哥\n", "playerId": "p15", "info": "军哥是路人王当之无愧的人气第一人，拥有娴熟的运球和精湛的进攻技巧，常常上演各种超高难度的进球，是路人王小个子球员的代表。" }, "p16": { "name": "安云鹏", "hwa": [191, 88, 21], "title": "路人王15冠王-“平常心”\n", "playerId": "p16", "info": "“平常心”孟亚东是一位从路人王走出来并被大众所熟知的球员，从去年3月的第一次参赛到今年4月，他已经收获了14个冠军头衔。因华丽的球风被粉丝冠以路人王欧文的称号。" } }
